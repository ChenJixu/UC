import React from 'react';

export default function TypeHeader(props) {
  console.log(props)
  var handleClick = function (e) {
    var type = e.currentTarget.getAttribute('type');
    props.onHandleClick(type);
    };
  return (
    <span
      className="HeaderItem"
      onClick={handleClick}
    >{props.type}</span>
  )
}